@model IEnumerable<BugTracker.Models.Ticket>

@{
    ViewBag.Title = "SubmittedTickets";
    bool isAuth = User.IsInRole("Administrator") || User.IsInRole("Developer");
}

<br />


<div class="panel panel-danger">

    <div class="panel-heading h2">Tickets with New Notifications</div>

    @* Tickets with waiting notifications *@
    @{ Html.RenderAction("NewNotifications"); }

</div>


<div class="panel panel-warning">

    <div class="panel-heading h2">Tickets Assigned to</div>

    @* tickets working on *@
    @{ Html.RenderPartial("_PersonalTicketTable", Model); }

</div>


